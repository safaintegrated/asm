<script>
    //have to be placed here, else child window cannot locate this function
    function SetSelectedEmployee(EmpSysId, EmpId, EmpName) {
        EmployeeId.SetText(EmpId.toString());
        EmployeeName.SetText(EmpName.toString());
        EmployeeSystemId.SetText(EmpSysId.toString());
        PopupLookupEmployee.Hide();
    }

    function ParentSystemUserSelected(UsrSysId, EmpSysId, EmpId, EmpName)
    {  
        UserSystemId.SetText(UsrSysId.toString());
        EmployeeSystemId.SetText(EmpSysId.toString());
        EmployeeName.SetText(EmpName.toString());
        EmployeeId.SetText(EmpId.toString());
        PopupSearchSystemUser.Hide();
        LoadingPanel.SetText('Sistem sedang membuka kembali maklumat terakhir yang disimpan.  Sila tunggu');
        LoadingPanel.Show();
        cbp_Load.PerformCallback();
    }
</script>

